<Tabs tabs="{{tabs}}" bindAcceptIndex="handleAcceptIndex">
    <view wx:for="{{allOrderList}}" wx:key="order_id">
        <view class="order_no">
            <view class="order_no_text">订单编号</view>
            <view class="order_no_detail">{{item.order_number}}</view>
        </view>
        <view class="order_price">
            <view class="order_price_text">订单价格</view>
            <view class="order_price_detail">￥{{item.order_price}}</view>
        </view>
        <view class="order_date">
            <view class="order_date_text">订单日期</view>
            <view class="order_date_detail">{{item.create_time_cn}}</view>
        </view>
    </view>
</Tabs>