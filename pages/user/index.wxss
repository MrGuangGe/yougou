page {
  background-color: #edece8;
}
.user {
  height: 40vh;
  overflow: hidden;
  position: relative;
}
.user .avatar_bg {
  height: 120%;
  filter: blur(20rpx);
}
.user .userinfo {
  position: absolute;
  top: 33%;
  left: 52%;
  transform: translateX(-50%);
}
.user .userinfo .user_avatar {
  width: 166rpx;
  height: 166rpx;
  background-color: deeppink;
  border-radius: 50%;
}
.user .userinfo .user_name {
  text-align: center;
  font-size: 36rpx;
  color: #000;
}
.content .main {
  margin-top: 16rpx;
  padding: 0 16rpx;
}
.content .main .collect_item {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 0;
  background-color: #fff;
}
.content .main .collect_item navigator {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.content .main .collect_item navigator .collect_num {
  color: var(--themeColor);
}
.content .main .order_item {
  margin-top: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 0;
  background-color: #fff;
}
.content .main .order_item navigator {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.content .main .order_item navigator .order_icon {
  color: var(--themeColor);
}
